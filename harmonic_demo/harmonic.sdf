    <model name="camera">
      <static>true</static>
      <pose>-9 -7.5 15 0.0, 0.4, 1.38</pose>
      <link name="link">
        <pose>0.05 0.05 0.05 0 0 0</pose>
        <visual name="visual">
          <geometry>
            <box>
              <size>0.1 0.1 0.1</size>
